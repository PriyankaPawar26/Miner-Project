  <body>
    <% include adminHeader %>
    <!-- section  -->

    <div class="Add_Staff">
      <div class="add_staff_fprm">
        <form action="/" method="POST">
          <h2 class="animate__animated animate__fadeInRightBig">Add Staff</h2>
          <div class="inputBox">
            <input type="text" name="Name" required="required" />
            <span>Name</span>
            <i></i>
          </div>
          <div class="inputBox">
            <input type="email" name="email" required="required" />
            <span>Email</span>
            <i></i>
          </div>
          <div class="inputBox">
            <input type="text" name="Gender" required="required" />
            <span>Gender</span>
            <i></i>
          </div>
          <div class="inputBox">
            <input type="password" name="password" required="required" />
            <span>Password</span>
            <i></i>
          </div>
          <div class="inputBox">
            <input type="password" name="password" required="required" />
            <span>Conform Password</span>
            <i></i>
          </div>
          <br />
          <div class="inputBox">
            <h5>Profile Pic.</h5>
            <input class="file" type="file" value="Choose File" />
            <i></i>
          </div>
          <div class="inputBox">
            <input type="text" name="Address" required="required" />
            <span>Address</span>
            <i></i>
          </div>
          <div class="inputBox">
            <input type="text" name="Department" required="required" />
            <span>Department</span>
            <i></i>
          </div>
          <input type="submit" value="Add Staff" />
        </form>
      </div>
    </div>

    <!--            Script               -->
    <script>
      $(".course-btn").click(function () {
        $("nav ul .course-show").toggleClass("show");
        $("nav ul .first").toggleClass("rotate");
      });
      $(".subject-btn").click(function () {
        $("nav ul .subject-show").toggleClass("show1");
        $("nav ul .second").toggleClass("rotate");
      });
      $(".session-btn").click(function () {
        $("nav ul .session-show").toggleClass("show2");
        $("nav ul .third").toggleClass("rotate");
      });
    </script>
