  <body>
    <% include adminHeader %>

    <!-- section  -->

    <div class="Add_Staff">
      <div class="add_staff_fprm">
        <form action="/" method="POST">
          <h2>Add Subject</h2>
          <div class="inputBox">
            <input type="text" name="Name" required="required" />
            <span>Name</span>
            <i></i>
          </div>
          <div class="inputBox">
            <input type="text" name="Name" required="required" />
            <span>Staff</span>
            <i></i>
          </div>
          <div class="inputBox">
            <input type="text" name="Name" required="required" />
