<% include adminHeader %>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
  integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

<ul style="text-align: center; height: 60px;" class="nav">
   
<style>
    .table thead tr th{
        margin-left: 20px;

    }
</style>
    <div class="clr"></div>
    <div class="header">
      <div class="clr"></div>
    </div>
    <div class="content">
      <center>
        <h3>View & Manage Student</h3>
      </center>
      <table style="margin-left: 150px;" class="table">
          <thead>
            <tr>
              <th>Id</th>
              <th>Name</th>
              <th>Email</th>
              <th>Contact</th>
              <th>Date</th>
              <th>Address</th>
              
              <th>Gender</th>
              <th>Status</th>
              <th>Action</th>
              <th>Profile</th>
            </tr>
          </thead>
          <tbody>
            <% for (let row of stdDetails){ %>
            <tr>
              <td><%= row._id%></td>
              <td><%= row.name%></td>
              <td><%= row.email%></td>
              <td><%= row.contact%></td>
              <td><%= row.dt%></td>
              <td><%= row.address%></td>
              
              <td><%= row.gender%></td>
              <td><%= row.status%></td>
              <td> <img src="../profile/<%= row.caticonnm%>" /></td>

              <td>
                <a href="/admin/manageStudent?s=delete&regid=<%= row._id%>" style=" color:red;text-decoration: none;">Delete User</a>
              </td>
            </tr>
            <% } %>
          </tbody>
        </table>
    </div>
    
  </div>
